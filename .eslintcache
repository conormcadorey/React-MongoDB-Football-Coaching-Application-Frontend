000/players\";\n        console.log(\"THE TOKEN:\");\n        console.log(token);\n\n        axios.get(`${url}/allauth`, {\n            headers: {\n                \"Authorization\": token\n            }\n        })\n            .then( response => setPlayers(response.data))\n            .catch( error => console.log(error));\n    }, []);\n\n    /////////////////////////\n    const useStyles = makeStyles({\n        root: {\n          minWidth: 275,\n        },\n        pos: {\n          marginBottom: 18,\n        },\n      });\n\n    const classes = useStyles();\n    /////////////////////////\n\n    return (\n        <>\n        {players ? (\n        <>\n        <h1>Your team</h1>\n        {players.map(player => {\n            const { _id, name, position, number } = player;\n            return (\n                <Card key={player._id} variant=\"outlined\" className={`${classes.pos} ${classes.root}`}>\n                <CardContent style={{backgroundColor: \"#F4F4F4\"}}>\n                    <Typography variant=\"h6\" component=\"h2\">\n                        {name}\n                    </Typography>\n                    <Typography color=\"textSecondary\">\n                        {position}\n                    </Typography>\n                    <Typography variant=\"body2\" component=\"p\">\n                        {number}\n                    <br />\n                    </Typography>\n                </CardContent>\n                <CardActions>\n                    <Button size=\"small\">Edit player</Button>\n                    <Button size=\"small\"><DeleteIcon/></Button>\n                </CardActions>\n                </Card>\n            )\n        })}\n        </>\n    ) : (\n        <CircularProgress/>\n    )}\n    </>\n  );\n}","/Users/conormcadorey/Documents/headr-frontend/src/components/pages/MyTeam.js",[],"/Users/conormcadorey/Documents/headr-frontend/src/components/pages/Match.js",["72"],"import React, { useState, useEffect } from \"react\";\nimport Timer from \"./Timer\";\nimport SubmitModal from \"./SubmitMatch\";\n\nimport { makeStyles } from '@material-ui/core/styles';\nimport Card from '@material-ui/core/Card';\nimport CardActions from '@material-ui/core/CardActions';\nimport CardContent from '@material-ui/core/CardContent';\nimport Button from '@material-ui/core/Button';\nimport Typography from '@material-ui/core/Typography';\nimport LinearProgress from '@material-ui/core/LinearProgress';\n\n//import SettingsBackupRestoreIcon from '@material-ui/icons/SettingsBackupRestore';\n\nexport default function Match() {\n\n    const [title, setTitle] = useState(true);\n    const [pause, setPause] = useState(true);\n    const [myGoals, setMyGoals] = useState(0);\n    const [oppGoals, setOppGoals] = useState(0);\n\n    //function to return true/false if match is ongoing and change UI output\n \n     /////////////////////////\n     const useStyles = makeStyles({\n        root: {\n          minWidth: 275,\n        },\n        pos: {\n          marginBottom: 18,\n        },\n        loadingRoot: {\n            width: '100%',\n        },\n      });\n\n    const classes = useStyles();\n    /////////////////////////\n\n    return (\n        <>\n        <h1>{title} {title ? (\"Match: LIVE\"):(\"Match: LIVE(Paused)\")}</h1>\n        <div className=\"matchCard\">\n            <Card variant=\"outlined\" className={`${classes.pos} ${classes.root}`}>\n                <CardContent style={{backgroundColor: \"#F4F4F4\"}}>\n                <Typography align=\"center\" variant=\"h6\" component=\"h2\">\n                        Ballymena U9's v Tottenham \n                    </Typography>\n                    <Typography align=\"center\" variant=\"h1\" color=\"textSecondary\">\n                        {myGoals} - {oppGoals}\n                    </Typography>\n                    <Typography align=\"center\" component=\"h2\">\n                        <Timer/>\n                    </Typography>\n                    <Typography variant=\"body2\" component=\"p\">\n                    <div className={classes.loadingRoot}>\n                        <LinearProgress/>\n                        <LinearProgress color=\"secondary\" />\n                    </div>\n                    <br />\n                    </Typography>\n                </CardContent>\n                    <Button onClick={() => setMyGoals(myGoals + 1)} size=\"large\" style={{width: \"50%\"}}>\n                        <h3>Ballymena U9's GOAL </h3>\n                    </Button>\n                    <Button onClick={() => setOppGoals(oppGoals + 1)} size=\"large\" style={{width: \"50%\"}}>\n                        <h3>TOTTENHAM GOAL </h3>\n                    </Button>\n                <CardActions>\n                    <Button onClick={()=>{ setTitle(!title); setPause(!pause) }} size=\"medium\" fullWidth=\"true\">\n                        {pause ? (\"PAUSE MATCH\"):(\"RESUME MATCH\")}\n                        </Button>\n                </CardActions>\n                    <SubmitModal/>\n                </Card>\n        </div>\n        </>\n\n    );\n\n}","/Users/conormcadorey/Documents/headr-frontend/src/components/pages/Timer.js",["73"],"import React, {useState, useEffect } from \"react\";\n\nexport default function Timer() {\n\n    const [seconds, setSeconds] = useState(0);\n    const [isRunning, setIsRunning] = useState(true);\n    const [intervalId, setIntervalId] = useState(null); \n\n    //setInterval to increase count to once every second\n    useEffect(() => {\n        //if timer is running \n        if (isRunning) {\n        const id = window.setInterval(() => {\n            setSeconds(seconds => seconds+1); }, 1000);\n            setIntervalId(id);\n        //cleanup function \n        //timer is not running\n        return () => window.clearInterval(id); \n        } \n    }, [isRunning]);\n\n    //<h1>{seconds}</h1>\n\n    //format timer\n    var date = new Date(0);\n    date.setSeconds(seconds); // specify value for SECONDS here\n    var formattedTimer = date.toISOString().substr(14, 5);\n\n        return (\n            <div className=\"timer\">\n                \n                <h1>{formattedTimer}</h1>\n\n            {isRunning ? (\n                <button onClick={() => {\n                    setIsRunning(false);\n                }}>\n                    PAUSE\n                </button>\n            ) : (\n                <button onClick={() => {\n                    setIsRunning(true);\n                }}>\n                    PLAY\n                </button>\n            )\n            }\n                {/*\n                <button disabled={!isRunning} onClick={() => {\n                    setIsRunning(false);\n                    setSeconds(0);\n                }}>\n                    RESET\n                </button>\n                */}\n            </div>   \n        );\n}","/Users/conormcadorey/Documents/headr-frontend/src/components/pages/SubmitMatch.js",[],{"ruleId":"74","replacedBy":"75"},{"ruleId":"76","replacedBy":"77"},{"ruleId":"78","severity":1,"message":"79","line":1,"column":17,"nodeType":"80","messageId":"81","endLine":1,"endColumn":26},{"ruleId":"78","severity":1,"message":"82","line":3,"column":10,"nodeType":"80","messageId":"81","endLine":3,"endColumn":20},{"ruleId":"78","severity":1,"message":"83","line":11,"column":8,"nodeType":"80","messageId":"81","endLine":11,"endColumn":24},{"ruleId":"78","severity":1,"message":"84","line":53,"column":21,"nodeType":"80","messageId":"81","endLine":53,"endColumn":24},{"ruleId":"78","severity":1,"message":"79","line":1,"column":27,"nodeType":"80","messageId":"81","endLine":1,"endColumn":36},{"ruleId":"78","severity":1,"message":"85","line":7,"column":12,"nodeType":"80","messageId":"81","endLine":7,"endColumn":22},"no-native-reassign",["86"],"no-negated-in-lhs",["87"],"no-unused-vars","'useEffect' is defined but never used.","Identifier","unusedVar","'useHistory' is defined but never used.","'FormControlLabel' is defined but nev