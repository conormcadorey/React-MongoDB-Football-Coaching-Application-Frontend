e/${myTeam}?page=${pageNumber}&limit=5`, {\n                headers: {\n                    \"Authorization\": token\n                }\n            })\n            .then( res => {\n                setPlayers(p => [...p, ...res.data.results])\n                setLoading(false)\n                setEnd(res.data.results)\n            });\n        } catch(err) {\n            console.error(err);\n        }\n    };\n\n    const handleUpdate = () => {\n        players.length = 0;\n        getPlayers();\n    }\n\n    const useStyles = makeStyles((theme) => ({\n    root: {\n        minWidth: 275,\n    },\n    pos: {\n        marginBottom: 18,\n    },\n    formControl: {\n        marginTop: theme.spacing(2),\n        marginBottom: theme.spacing(2),\n        minWidth: 120,\n        },\n    }));\n\n    const classes = useStyles();\n\n    return (\n        <>\n        {players.length > 0 ? (\n        <>\n        <InfiniteScroll \n            dataLength={players.length} \n            next={() => \n                setPageNumber(pageNumber + 1)\n            } \n            hasMore={true}\n            >\n        {players.map(player => {\n            const { name, position, number, _id, userId } = player || {};\n            return (\n                <Zoom>\n                <Card variant=\"outlined\" key={player._id} className={`${classes.pos} ${classes.root}`}>\n                    <Box display=\"flex\" pt={1} style={{backgroundColor: \"#F4F4F4\"}}>\n                        <Box p={1} flexGrow={1}>\n                            <CardContent>\n                                <Typography variant=\"h5\" component=\"h2\">\n                                    {name}\n                                </Typography>\n                                <Typography color=\"textSecondary\">\n                                    Position | {position} \n                                </Typography>\n                                <Typography variant=\"body2\" component=\"p\">\n                                    Number | {number} \n                                <br />\n                                </Typography>\n                            </CardContent>\n                        </Box>\n                        <Box p={1}>\n                            <CardContent>\n                                <Avatar src=\"/broken-image.jpg\" style={{ height: '70px', width: '70px' }}/>\n                            </CardContent>\n                        </Box>\n                    </Box>\n                    <Box display=\"flex\" justifyContent=\"flex-end\">\n                        <CardActions>\n                            {userId === myId ? \n                            (\n                            <>\n                            <EditPlayerDialog \n                                name={name} \n                                position={position} \n                                number={number} \n                                id={_id} \n                                onUpdate={() => handleUpdate()}\n                            />\n                            <DeletePlayerDialog \n                                name={name} \n                                id={_id} \n                                players={players}\n                                onUpdate={(id) => {\n                                    const newPlayers = players.filter(player => player._id !== id)\n                                    setPlayers(newPlayers)\n                                }}\n                            />\n                            </>\n                            ) : (\n                            <>\n                            <Tooltip title=\"Cannot edit this player. Update your Admin privileges\" arrow>\n                            <span>\n                                <Button \n                                disabled\n                                size=\"large\"\n                                >\n                                    <MoreVertIcon/>\n                                </Button>\n                            </span>\n                            </Tooltip>\n\n                            <Tooltip title=\"Cannot delete this player. Update your Admin privileges\" arrow>\n                            <span>\n                                <Button disabled size=\"large\">\n                                    <DeleteIcon/>\n                                </Button>\n                            </span>\n                            </Tooltip> \n                            </>\n                        )}   \n                    </CardActions>\n                </Box>\n            </Card>\n            </Zoom>\n            )\n        })}\n        </InfiniteScroll>\n\n        <Card variant=\"outlined\" style={{backgroundColor: \"#FFF\", padding: \"1rem\", marginBottom: \"1rem\"}}>\n            {!loading && \n            <Typography align=\"center\" variant=\"body2\">\n                {players.length} players\n            </Typography>  \n            }\n\n            {loading && \n            <Typography align=\"center\" variant=\"body2\">\n                <CircularProgress />\n            </Typography>  \n            }\n\n            {end.length === 0 && \n            <Typography align=\"center\" variant=\"body2\">\n                End of results\n            </Typography>  \n            }\n        </Card>\n\n        </>\n        ) : (\n            <Card variant=\"outlined\" className={`${classes.pos} ${classes.root}`}>\n                <CardContent pt={1} style={{backgroundColor: \"#F4F4F4\", padding: \"2rem\"}}>\n                    <Typography align=\"center\" variant=\"body2\">\n                        <CircularProgress />\n                        <br></br><br></br>\n                        You currently have no registered players. <Link to=\"/createplayer\" style={{color: \"black\"}}>Click here to create one.</Link>\n                    </Typography>   \n                </CardContent>\n            </Card>\n        )\n        }\n        </>\n    );\n}\n","/Users/conormcadorey/Documents/headr-frontend/src/components/pages/MyTeam.js",[],"/Users/conormcadorey/Documents/headr-frontend/src/components/pages/Match.js",[],"/Users/conormcadorey/Documents/headr-frontend/src/components/pages/Timer.js",[],"/Users/conormcadorey/Documents/headr-frontend/src/components/pages/SubmitMatch.js",[],"/Users/conormcadorey/Documents/headr-frontend/src/components/pages/NewMatchDialog.js",[],"/Users/conormcadorey/Documents/headr-frontend/src/components/pages/Footer.js",[],"/Users/conormcadorey/Documents/headr-frontend/src/components/pages/CreatePlayer.js",[],"/Users/conormcadorey/Documents/headr-frontend/src/redux/configureStore.js",[],"/Users/conormcadorey/Documents/headr-frontend/src/redux/todoSlice.js",[],"/Users/conormcadorey/Documents/headr-frontend/src/redux/oppositionSlice.js",[],"/Users/conormcadorey/Documents/headr-frontend/src/components/pages/PauseTimer.js",[],"/Users/conormcadorey/Documents/headr-frontend/src/redux/timerSlice.js",[],"/Users/conormcadorey/Documents/headr-frontend/src/components/pages/EditPlayerDialog.js",[],"/Users/conormcadorey/Documents/headr-frontend/src/components/pages/DeletePlayerDialog.js",[],"/Users/conormcadorey/Documents/headr-frontend/src/components/pages/Fixtures.js",[],"/Users/conormcadorey/Documents/headr-frontend/src/components/pages/MyAccount.js",[],"/Users/conormcadorey/Documents/headr-frontend/src/components/pages/ChangePassword.js",[],"/Users/conormcadorey/Documents/headr-frontend/src/components/pages/SelectTeam.js",[],["169","170"],"/Users/conormcadorey/Documents/headr-frontend/src/components/pages/UpcomingFixtures.js",["171"],"import React, { useState, useEffect } from \"react\";\nimport { Link, useHistory } from \"react-router-dom\";\nimport axios from \"axios\";\nimport moment from \"moment\";\nimport DeleteMatchDialog from \"./DeleteMatchDialog\";\n\nimport { useDispatch } from \"react-redux\";\nimport { addOpposition } from \"./../../redux/oppositionSlice\