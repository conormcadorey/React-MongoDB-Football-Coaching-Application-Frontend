ot}`}>\n                <CardContent style={{backgroundColor: \"#F4F4F4\"}}>\n                    <Typography variant=\"h6\" component=\"h2\">\n                        {name}\n                    </Typography>\n                    <Typography color=\"textSecondary\">\n                        {position}\n                    </Typography>\n                    <Typography variant=\"body2\" component=\"p\">\n                        {number}\n                    <br />\n                    </Typography>\n                </CardContent>\n                <CardActions>\n                    <Button size=\"small\">Edit player</Button>\n                    <Button size=\"small\"><DeleteIcon/></Button>\n                </CardActions>\n                </Card>\n            )\n        })}\n        </>\n    ) : (\n        <CircularProgress/>\n    )}\n    </>\n  );\n}","/Users/conormcadorey/Documents/headr-frontend/src/components/pages/MyTeam.js",[],"/Users/conormcadorey/Documents/headr-frontend/src/components/pages/Match.js",[],"/Users/conormcadorey/Documents/headr-frontend/src/components/pages/Timer.js",["109"],"/Users/conormcadorey/Documents/headr-frontend/src/components/pages/SubmitMatch.js",[],"/Users/conormcadorey/Documents/headr-frontend/src/components/pages/NewMatchDialog.js",["110"],"/Users/conormcadorey/Documents/headr-frontend/src/components/pages/Footer.js",[],"/Users/conormcadorey/Documents/headr-frontend/src/components/pages/CreatePlayer.js",[],"/Users/conormcadorey/Documents/headr-frontend/src/redux/configureStore.js",[],"/Users/conormcadorey/Documents/headr-frontend/src/redux/todoSlice.js",[],"/Users/conormcadorey/Documents/headr-frontend/src/redux/oppositionSlice.js",[],"/Users/conormcadorey/Documents/headr-frontend/src/components/pages/PauseTimer.js",[],"/Users/conormcadorey/Documents/headr-frontend/src/redux/timerSlice.js",[],"/Users/conormcadorey/Documents/headr-frontend/src/components/pages/RedirectLogin.js",["111"],{"ruleId":"112","severity":1,"message":"113","l